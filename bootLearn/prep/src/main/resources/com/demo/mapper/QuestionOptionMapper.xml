<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.QuestionOptionMapper">
    <!-- 分页查询-->
    <select id="getByPage" resultType="com.demo.model.entity.QuestionOption">
        SELECT 
        option_id, question_id, op_content, is_correct, op_no, op_name
        FROM question_option
    </select>
</mapper>
