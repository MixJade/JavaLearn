<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.ImageSourceMapper">
    <!-- 分页查询-->
    <select id="getByPage" resultType="com.demo.model.entity.ImageSource">
        SELECT 
        source_id, file_name, category_id, remark, ocr_time
        FROM image_source
    </select>
</mapper>
